{
  "name": "nostromo",
  "version": "0.0.18",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run version-build",
    "build": "ng build",
    "build-dev": "ng build",
    "build-public": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pre-deploy": "npm version patch && git add . ",
    "deploy-stage": "npm run build && cd deployment && npm run deploy",
    "deploy-dev": "npm run build && cd deployment && npm run go-live",
    "version-build": "npm version patch && node ./version.build.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run version-build && git add src/environments/version.ts && git commit -m $npm_package_version"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "husky": "^1.2.0",
    "replace-in-file": "^3.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
